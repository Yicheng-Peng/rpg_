[sub_resource type="Resource" id="Resource_mhbqv"]
script = ExtResource("1_d188u")
name = "Leafy"
content = "This forest is your first trial.
Inside, you'll find treasure chests that can boost your abilities, so don’t just rush straight to the second stage!
Without those upgrades, you’ll have a hard time defeating the Demon Lord!"
avatar = ExtResource("2_ss4m8")
show_on_left = false
metadata/_custom_type_script = "uid://culsjjboak18s"

[sub_resource type="Resource" id="Resource_h0lhr"]
script = ExtResource("1_d188u")
name = "Leafy"
content = "—— And also!
There are a bunch of little slimes wandering around.
They’re not strong individually, but they come in numbers.
Here, take this sword!
Open your backpack and equip it —
Otherwise, you might get overwhelmed by the slime horde!"
avatar = ExtResource("2_ss4m8")
show_on_left = false
metadata/_custom_type_script = "uid://culsjjboak18s"

[sub_resource type="Resource" id="Resource_l02ma"]
script = ExtResource("1_d188u")
name = "Leafy"
content = "The slimes patrol the area, and once they spot you, they’ll rush at you!
Be careful!"
avatar = ExtResource("2_ss4m8")
show_on_left = false
metadata/_custom_type_script = "uid://culsjjboak18s"

[sub_resource type="Resource" id="Resource_1ghjr"]
